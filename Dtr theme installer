#!/bin/bash

main() {
    cd /Applications/MacSploit.app/Contents/Resources/CodeViewer_CodeViewer.bundle/Contents/Resources/ace.bundle
    echo -e "Installing Theme..."
    curl "https://raw.githubusercontent.com/MoonP13/Moon/main/dtr%20theme.1" -o "./theme-tomorrow_night.js"
    echo -e "Theme installation is now complete!"
}

main
